<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    package="denis.stepanov">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DenisActivity"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/search"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName=".DenisActivity" />
        <activity
            android:name=".AddPatientActivity"
            android:label="@string/patient"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName=".DenisActivity"></activity>
        <activity android:name=".AddTestActivity"
            android:label="@string/test"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName=".DenisActivity"></activity>
    </application>

</manifest>