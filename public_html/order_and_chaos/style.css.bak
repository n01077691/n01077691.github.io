#p_standard{
	height: 6%;
	width:	24%;
	position: absolute;
	top: 25%;
	left: 0.5%;
	border: 1px solid yellow;
	text-align: center;
	font-size: 26px;
	font-weight: bold;
	color: #F781BE;
	text-shadow: 0px 0px 2px #F781BE;
	
}
#p_order{
	height: 13%;
	width:	24%;
	position: absolute;
	left: 0.5%;
	top: 40%;
	border: 1px solid yellow;
	text-align: center;
	font-size: 22px;
	font-weight: bold;
	color: #F781BE;
	text-shadow: 0px 0px 2px #F781BE;
}
#p_chaos{
	height: 10%;
	width:	24%;
	position: absolute;
	top: 60%;
	left: 0.5%;
	border: 1px solid yellow;
	text-align: center;
	font-size: 22px;
	font-weight: bold;
	color: #F781BE;
	text-shadow: 0px 0px 2px #F781BE;
}
pre{
	height: 90%;
	width:	24.5%;
	position: absolute;
	top: 0.5%;	
	right: 0.5%;
	border: 1px solid yellow;
	/*text-align: center;*/
	font-size: 20px;
	font-weight: bold;
	color: #F781BE;
	text-shadow: 0px 0px 2px #F781BE;
}


#p_restart{
	height: 6%;
	width:	24%;
	position: absolute;
	bottom: 2%;
	left: 0.5%;
	border: 1px solid yellow;
	text-align: center;
	font-size: 25px;
	font-weight: bold;
	color: #FAAC58;
	text-shadow: 0px 0px 2px #FAAC58;
}
#p_restart:hover{
	color: #F781BE;
	text-shadow: 0px 0px 2px #F781BE;
}

  
