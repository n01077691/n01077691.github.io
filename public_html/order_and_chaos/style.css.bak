/*style.css*/
 body {
	background-color: #424242;
	min-width: 85em;
}
.order{
	background: blue;
	height: 100%;
	width: 100%;
	border-radius: 50%;
	background: radial-gradient(#A9A9F5, #5858FA, #0000FF);
}

.chaos{
	background: red;
	height: 100%;
	width:  100%;
	border-radius: 50%;
	background: radial-gradient(#F6CECE, #FA5858, #FF0000);
}

.relative{
	display: inline-block;
	height: 16%;
	width: 8%;
	border-radius: 50%;
	position: absolute;
	left: 2.5%;
	top: 5%;
	background: radial-gradient(#A9A9F5, #5858FA, #0000FF);
	box-shadow: 0 0 20px 5px #0000FF;
}
.relative_red{
	display: inline-block;
	height: 16%;
	width: 8%;
	border-radius: 50%;
	position: absolute;
	left: 15%;
	top: 5%;
	background: radial-gradient(#F6CECE, #FA5858, #FF0000);
	box-shadow: 0 0 20px 5px #FF0000;
}
/*Selection ball classes*/
.select_red{
box-shadow: 0 0 50px 15px #FF0000, 0 0 55px 20px #FFF933;
}

.select_blue{
box-shadow: 0 0 50px 15px #0000FF, 0 0 55px 20px #33DDFF;
}
/****************************/

.relative:hover {
    box-shadow: 0 0 50px 15px #0000FF, 0 0 55px 20px #33DDFF;
}

.relative_red:hover{
    box-shadow: 0 0 50px 15px #FF0000, 0 0 55px 20px #FFF933;
}

table {
	border-spacing: 0px; 
	width: 48%;
	height: 95%;
	position: absolute;
	left: 26%;
	top: 3%;
	border: 5px solid #FAAC58;
	border-radius: 10px;
	box-shadow: 0 0 15px 5px #FAAC58;	
}
td {
    -webkit-box-sizing: content-box;
    -moz-box-sizing: content-box;
    -ms-box-sizing: content-box;
    box-sizing: content-box;
    -ms-interpolation-mode: bicubic;
	border: 4px solid #FAAC58;
	border-radius: 2px;
	box-shadow: 0 0 5px 5px #FAAC58;
}
.tablecell {
	height: 100%;
	width:  100%;
}
