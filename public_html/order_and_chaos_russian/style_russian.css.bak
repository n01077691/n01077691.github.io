/*style.css*/
 body {
	background-color: #424242;
	overflow: hidden;
	padding: 0px;
	margin: 0px;
	height: 500px;
}

.order{
	background: blue;
	height: 100px;
	width: 100px;
	border-radius: 50px;
	background: radial-gradient(#A9A9F5, #5858FA, #0000FF);
}

.chaos{
	background: red;
	height: 100px;
	width:  100px;
	border-radius: 50px;
	background: radial-gradient(#F6CECE, #FA5858, #FF0000);
}

.relative{
    height: 100px;
    width: 100px;
	border-radius: 50px;
	position: relative;
	left: 220px;
	top:  15px;
	background: radial-gradient(#A9A9F5, #5858FA, #0000FF);
	box-shadow: 0 0 20px 5px #0000FF;
}
.relative_red{
    height: 100px;
    width: 100px;
	position: relative;
	bottom:120px;
	left:20px;
	border-radius: 50px;
	background: radial-gradient(#F6CECE, #FA5858, #FF0000);
	box-shadow: 0 0 20px 5px #FF0000;
}
/*Selection ball classes*/
.select_red{
box-shadow: 0 0 50px 15px #FF0000, 0 0 55px 20px #FFF933;
}

.select_blue{
box-shadow: 0 0 50px 15px #0000FF, 0 0 55px 20px #33DDFF;
}
/****************************/

.relative:hover {
    box-shadow: 0 0 50px 15px #0000FF, 0 0 55px 20px #33DDFF;
}

.relative_red:hover{
    box-shadow: 0 0 50px 15px #FF0000, 0 0 55px 20px #FFF933;
}

table {
	border-spacing: 0px; 
	position: absolute;
	left: 370px;
	top: 20px;
	border: 5px solid #FAAC58;
	border-radius: 10px;
	box-shadow: 0 0 15px 5px #FAAC58;	
}
td {
	width: 85px;
	height: 85px;
    vertical-align: middle;
    -webkit-box-sizing: content-box;
    -moz-box-sizing: content-box;
    -ms-box-sizing: content-box;
    box-sizing: content-box;
    -ms-interpolation-mode: bicubic;
	border: 4px solid #FAAC58;
	border-radius: 2px;
	box-shadow: 0 0 5px 5px #FAAC58;
}

form {
	position: absolute;
	bottom: 0px;
	right: 0px;
	padding: 15px;
	background-color: rgb(83, 175, 19);
}
form input {
	background-color: rgb(152, 207, 113);
	border-color: rgb(83, 175, 19);
	font-size: 1em;
}

.tablecell {
	height: 100%;
	width:  100%;
}
.tablecell:hover  {
    /*box-shadow: 0 0 50px 15px #f71570;*/
	box-shadow: inset 0 0 50px 20px #F781BE;
    -webkit-transition: all 200ms ease;
    -moz-transition: all 200ms ease;
    -ms-transition: all 200ms ease;
    -o-transition: all 200ms ease;
    transition: all 200ms ease;
  }
